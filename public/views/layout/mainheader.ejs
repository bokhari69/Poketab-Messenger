    *{
        padding: 0;
        margin: 0;
        box-sizing: border-box;
        font-family: 'Comic';
    }

    #preload{
        z-index: 10000; 
        position: fixed; 
        top: 0; 
        left: 0; 
        bottom: 0; 
        right: 0; 
        display:flex; 
        flex-direction:row; 
        gap: 5px; 
        align-items:center; 
        justify-content:center; 
        background: var(--primary-dark, #111d2a); 
    }
    #preload .text{
        color: var(--secondary-dark, hsl(213, 98%, 57%));
    }
    #preload .text i{
        color: var(--secondary-dark, hsl(213, 98%, 57%));
    }
